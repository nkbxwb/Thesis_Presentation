\begin{frame}{Data Explanation}
\begin{itemize}
 \item 1514 MD Anderson patients who had brain mets from breast cancer between October 2009 and 
 December 2012
 \item 1242 usable cases
 \item 90 covariates
 \begin{itemize}
  \item Missingness from 0 to 65\%
 \end{itemize}

\end{itemize}
